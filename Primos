public class Primos{
    public static void main(String[] args){
        for (int i = 2; i <= 2000; i++){
            if(primo(i)){
                System.out.println(i);
            }
        }
        }
    public static boolean primo(int numero){
        if(numero <=1){
            return false;
        }
        for (int i = 2; i <= Math.sqrt(numero); i++){
            if(numero % i ==0){
                return false;
            }
        }
        return true;
    }
    }
